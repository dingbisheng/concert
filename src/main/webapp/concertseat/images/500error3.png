<div class="content">
	<img id="img404" src="" >
    <div class="txt">
      <a class="index" href="//www.damai.cn/">回首页</a>
      <a class="index" href="//event.damai.cn/">抢免费票</a>
      <a class="index" href="//movie.damai.cn/">看特价电影</a>
    </div>
</div>

<script src="/js/utlis.js"></script>
<script>
(function (win, doc) {
  win.onload = function () {
    var data = [{
      title: '缅怀John Lennon-大麦网404',
      image: '/img/john.jpg'
    }, {
      title: '缅怀邓丽君-大麦网404',
      image: '/img/teresa.jpg'
    }, {
      title: '缅怀黄家驹-大麦网404',
      image: '/img/hjg.jpg'
    }, {
      title: '缅怀张国荣-大麦网404',
      image: '/img/cheung.jpg'
    }];

    var cookieNum = utlis.cookie.get('page404.num');
    var num = utlis.random(0, data.length - 1);

    if (!cookieNum || cookieNum.split(' ').length < data.length) {
      while (cookieNum && cookieNum.indexOf(num) !== -1) {
        num = utlis.random(0, data.length - 1);
      }

      var arr = [];

      cookieNum && (arr = cookieNum.split(' '));
      
      arr.push(num);
      
      cookieNum = arr.join(' ');

      utlis.cookie.set('page404.num', cookieNum, 1000000);
    }
    
    var img404 = doc.getElementById('img404');

    img404.src = data[num].image;

    doc.title = data[num].title;
  };

  
})(window, document);
</script>
